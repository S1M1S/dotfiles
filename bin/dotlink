#!/bin/bash

rm -r $HOME/.oh-my-zsh
rm -rf $HOME/.vim
rm $HOME/.vimrc
rm $HOME/.zshrc
rm -r $HOME/bin

ln -s $HOME/dotfiles/.oh-my-zsh $HOME/.oh-my-zsh
ln -s $HOME/dotfiles/.vim $HOME/.vim
ln -s $HOME/dotfiles/.vimrc $HOME/.vimrc
ln -s $HOME/dotfiles/.zshrc $HOME/.zshrc
ln -s $HOME/dotfiles/bin $HOME/bin
